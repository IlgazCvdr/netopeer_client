<ns0:config xmlns:ns0="urn:ietf:params:xml:ns:netconf:base:1.0" xmlns:ns1="urn:ietf:params:xml:ns:yang:ietf-keystore" xmlns:ns2="urn:ietf:params:xml:ns:yang:ietf-netconf-acm" xmlns:ns3="urn:ietf:params:xml:ns:yang:ietf-netconf-server" xmlns:ns4="urn:cesnet:libnetconf2-netconf-server" xmlns:ns5="test1" xmlns:ns6="test2">
  <ns1:keystore>
    <ns1:asymmetric-keys>
      <ns1:asymmetric-key>
        <ns1:name>{$1}</ns1:name>
        <ns1:public-key-format>{$2}</ns1:public-key-format>
        <ns1:private-key-format>{$3}</ns1:private-key-format>
        <ns1:cleartext-private-key>{$4}</ns1:cleartext-private-key>
      </ns1:asymmetric-key>
    </ns1:asymmetric-keys>
  </ns1:keystore>
  <ns2:nacm>
    <ns2:enable-nacm>{$5}</ns2:enable-nacm>
  </ns2:nacm>
  <ns3:netconf-server>
    <ns3:listen>
      <ns3:endpoints>
        <ns3:endpoint>
          <ns3:name>{$6}</ns3:name>
          <ns3:ssh>
            <ns3:tcp-server-parameters>
              <ns3:local-address>{$7}</ns3:local-address>
            </ns3:tcp-server-parameters>
            <ns3:ssh-server-parameters>
              <ns3:server-identity>
                <ns3:host-key>
                  <ns3:name>{$8}</ns3:name>
                  <ns3:public-key>
                    <ns3:central-keystore-reference>{$9}</ns3:central-keystore-reference>
                  </ns3:public-key>
                </ns3:host-key>
              </ns3:server-identity>
              <ns3:client-authentication>
                <ns3:users>
                  <ns3:user>
                    <ns3:name>{$10}</ns3:name>
                    <ns4:keyboard-interactive>
                      <ns4:use-system-auth>{$11}</ns4:use-system-auth>
                    </ns4:keyboard-interactive>
                  </ns3:user>
                </ns3:users>
              </ns3:client-authentication>
            </ns3:ssh-server-parameters>
          </ns3:ssh>
        </ns3:endpoint>
      </ns3:endpoints>
    </ns3:listen>
  </ns3:netconf-server>
  <ns5:people>
    <ns5:person>
      <ns5:name>{$12}</ns5:name>
      <ns5:phone>{$13}</ns5:phone>
    </ns5:person>
    <ns6:person>
      <ns6:name>{$14}</ns6:name>
      <ns6:phone>{$15}</ns6:phone>
    </ns6:person>
  </ns5:people>
</ns0:config>